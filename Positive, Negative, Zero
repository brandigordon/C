#include <iostream>

using namespace std;

int main() {
    // Write your main here


  int number = 0;

  cout << "Enter a number: ";
  cin >> number;
  if (number > 0)
    cout << "Positive" <<endl;
  else if (number < 0)
    cout << "Negative" <<endl;
  else
    cout << "Number is Zero"<<endl;



    return 0;
}
